{
  "actions" : {
    "build" : {
      "enabled" : true,
      "script" : "bundle update\nbundle install\nJEKYLL_ENV=production bundle exec jekyll build"
    },
    "clean" : {
      "enabled" : true,
      "script" : "bundle exec jekyll clean"
    },
    "run" : {
      "enabled" : true,
      "script" : "JEKYLL_ENV=staging bundle exec jekyll serve --liveReload -o"
    }
  },
  "openLogOnRun" : "start"
}
